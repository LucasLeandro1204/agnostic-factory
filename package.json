{
  "name": "agnostic-factory",
  "version": "0.1.0",
  "description": "Agnostic Factory to make your life easier when testing Edit",
  "main": "dist/Factory",
  "scripts": {
    "build": "rm -r /dist -f && babel src --out-dir dist --copy-files",
